local farm = false

_G.Config = { UserID = "a6eb4e69-060d-42b0-99b5-834601bb6da2", discord_id = "332193090825748484" , Note = "sell", } 
task.spawn(function()
		task.wait(70)
	loadstring(game:HttpGet("https://raw.githubusercontent.com/skadidau/unfazedfree/refs/heads/main/petsgo"))()
end)

 if not game:IsLoaded() then
    game.Loaded:Wait()
  end
  
if farm then
	repeat
	wait()
		task.spawn(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/bomayso/ram/refs/heads/main/petg0nomail"))()
		end)
	wait(3)
	until daubuoi
else
	repeat
	wait()
		task.spawn(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/bomayso/ram/refs/heads/main/sellpetgo"))()
		end)
	wait(3)
	until daubuoi
end
